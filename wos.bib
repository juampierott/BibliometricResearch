@article{ WOS:001259304400001,
Author = {Gourabpasi, Arash Hosseini and Nik-Bakht, Mazdak},
Title = {An ontology for automated fault detection \& diagnostics of HVAC using
   BIM and machine learning concepts},
Journal = {SCIENCE AND TECHNOLOGY FOR THE BUILT ENVIRONMENT},
