@article{ WOS:000595260600013,
Author = {He, Rui and Li, Mingkai and Gan, Vincent J. L. and Ma, Jun},
Title = {BIM-enabled computerized design and digital fabrication of
   industrialized buildings: A case study},
Journal = {JOURNAL OF CLEANER PRODUCTION},
Year = {2021},
Volume = {278},
Month = {JAN 1},
Abstract = {Industrialized construction leverages factory-based manufacturing and
   lean-site assembly to achieve higher industrial efficiency. Building
   information modeling (BIM) offers new opportunities to underpin the
   computerized design and fabrication of industrialized buildings,
   providing greater productivity and cost-effectiveness. In this study,
   attempts are made to review the state-of-the-art BIM applications on
   design and prefabrication automation of industrialized buildings, with
   more emphasis on the recent achievement in concrete 3D printing
   technology. Following this, a BIM method is proposed to support the
   detailed geometry design and digital fabrication of modular housings. A
   program interfaced within BIM is developed and demonstrated through
   discussions on its capacity in generating the geometry details of
   3D-printed modules. In addition, robotic simulation of 3D printing is
   performed to explore a flexible plan in producing the 3D-printed modules
   or components. The lessons learned from the study are discussed to
   highlight the future research challenges and opportunities. First, as
   current Industry Foundation Classes-based BIM model provides limited
   support to prefabrication, research on BIM open standards demands more
   attentions to interoperate the computerized design and computer-aided
