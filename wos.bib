Language = {English},
Affiliation = {Wang, DJ (Corresponding Author), Shanghai Univ, Sch Mech \& Engn Sci, Dept Civil Engn, Shanghai 200444, Peoples R China.
   Wang, Dejiang; Liu, Bolong, Shanghai Univ, Sch Mech \& Engn Sci, Dept Civil Engn, Shanghai 200444, Peoples R China.
   Jiang, Haili; Liu, Panpan, Shanghai Highway \& Bridge Grp Co Ltd, Shanghai 200433, Peoples R China.},
DOI = {10.3390/buildings15050719},
