@article{ WOS:001425793900001,
Author = {Pandimani and Bilgates, P. and Raviteja, Y. and Akhil, P. D. V. S. and
   Sowmya, Y.},
Title = {Seismic resilience and energy optimization of educational RC buildings
   using BIM: a sustainable design approach},
Journal = {ENGINEERING RESEARCH EXPRESS},
Year = {2025},
Volume = {7},
Number = {1},
Month = {MAR 31},
Abstract = {Building information modeling (BIM) allows structures to be digitally
   designed and visualized before physical construction begins, effectively
   addressing and eliminating many inefficiencies and issues that typically
   arise during the construction process. This research evaluates the
   seismic design and energy consumption of Reinforced Concrete (RC)
   buildings using different BIM tools. The impact of different building
   configurations (including window-wall ratio, shading, and automation
   sensor effects) and orientation on the electrical energy consumption are
   evaluated. Integrating these factors into energy analysis can enhance
   conceptual design, leading to better environmental effectiveness and
   aiding in the achievement of environmental sustainability goals. The
   study determined that aligning the building at true north (zero
   degrees), using two-thirds window shade height, maintaining a 0\%
   window-wall ratio, and integrating day-lighting and occupancy sensors
   resulted in the most effective energy optimization outcomes.
   Optimization resulted in a reduction of overall energy consumption from
   975 MJ/m2/year to 729 MJ/m2/year, demonstrating that the BIM model is
   25\% more efficient than the actual building envelope. The sustainable
   design alternative achieves a 36.84\% savings in lifecycle energy costs.
   The findings accentuate the need for adopting passive designs to improve
   energy efficiency, cost, and sustainability.},
