@article{ WOS:001189049600001,
Author = {Prieto, Samuel A. and Giakoumidis, Nikolaos and de Soto, Borja Garcia},
Title = {Multiagent robotic systems and exploration algorithms: Applications for
   data collection in construction sites},
Journal = {JOURNAL OF FIELD ROBOTICS},
