@article{ WOS:001256071900001,
Author = {Feist, Sofia and Jacques de Sousa, Luis and Sanhudo, Luis and Pocas
   Martins, Joao},
Title = {Automatic Reconstruction of 3D Models from 2D Drawings: A
   State-of-the-Art Review},
Journal = {ENG},
Year = {2024},
Volume = {5},
Number = {2},
Pages = {784-800},
Month = {JUN},
Abstract = {Among the methods of 3D reconstruction, the automatic generation of 3D
   models from building documentation is one of the most accessible and
   inexpensive. For 30 years, researchers have proposed multiple methods to
   automatically generate 3D models from 2D drawings. This study compiles
   this research and discusses the different methods used to generate 3D
   models from 2D drawings. It offers a critical review of these methods,
   focusing on the coverage and completeness of the reconstruction process.
   This review allows us to identify the research gaps in the literature,
