@article{ WOS:001420653300001,
Author = {Guo, Ming and Wu, Xiangling and Qi, Huihui and Zhang, Ying and Chen,
   Jiangjihong and Wei, Yaxuan and Zhang, Xiaolan and Shang, Xiaoke},
Title = {A methodology for sky-space-ground integrated remote sensing monitoring:
   A digital twin framework for multi-source Data-BIM integration in
   residential quality monitoring},
Journal = {JOURNAL OF BUILDING ENGINEERING},
Year = {2025},
Volume = {102},
Month = {MAY 15},
Abstract = {This study introduces an innovative data-BIM integrated digital twin
   framework for the quality monitoring residential developments. The
   framework amalgamates residential building information modeling (RBIM),
   surveying sensors and Internet of Things (SSIoT) technologies to
   establish a multi-faceted and multi-scale monitoring system. Within this
   framework, RBIM serves as a residential information twin model that
   aggregates comprehensive life cycle data of residential projects,
   encompassing design, building, operation, and maintenance. The physical
   things are linked to cyberspace via SSIoT technology, significantly
   improving data exchange and processing capabilities. To address the
   limitations of singular monitoring technology, a BIM collaborative
   multi-source data monitoring method for residential engineering quality
   is proposed, utilizing the BIM model alongside the advancement of
   real-time transmitting monitoring LiDAR, automated monitoring robots,
   GNSS, and various other sensors. This framework seeks to create a novel
   paradigm for intelligent, comprehensive data management and synthesizing
   heterogeneous data from several sources, focusing on the quality
   monitoring of residential projects to guarantee quality, safety, and
   operational efficiency.},
