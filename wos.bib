@article{ WOS:001087726300001,
Author = {Li, Cui and Lu, Ping and Zhu, Weiran and Zhu, Han and Zhang, Xinmin},
Title = {Intelligent Monitoring Platform and Application for Building Energy
   Using Information Based on Digital Twin},
