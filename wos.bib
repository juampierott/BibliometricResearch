@article{ WOS:001256791500001,
Author = {Guo, Zeng and Li, Qianyao and Wang, Qiankun and Qiao, Shi and Mei,
   Tingting and Zuo, Weiwei},
Title = {Reducing redundancy in large MEP building information models for
   facility management using oriented bounding boxes and hash sets},
Journal = {AUTOMATION IN CONSTRUCTION},
Year = {2024},
Volume = {165},
Month = {SEP},
Abstract = {The building information models of mechanical, electrical, and plumbing
   (MEP) systems often contain numerous identical elements, resulting in
   significant data redundancy. Current redundancy-removal techniques often
   involve intricate computational procedures and data conversions, which
   is time-consuming and error-prone. To overcome this limitation, this
   paper introduces an easy but efficient approach for mitigating data
   redundancy in large MEP models by integrating Oriented Bounding Boxes
   (OBB) and Hash sets. To validate its efficacy, 16 actual Revit MEP
   project are used to test the compression capacity and accuracy of the
   method. The results indicate that, using this approach, the sizes of the
   models can be reduced to only 4\% and 33\% of the original Revit and
   lightweight Navisworks counterparts, respectively. Meanwhile, the
   maximum error of the restored model is below 1 mm. This paper not only
   introduces a compression technique for MEP models but also paves the way
   for smoother BIM implementation in facility management.},
