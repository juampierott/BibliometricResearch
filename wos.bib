@article{ WOS:001448089600001,
Author = {Aldrees, Rashed M. and Al-Gahtani, Khalid S. and Alsugair, Abdullah M.
   and Aljadhai, Saad I. and Alsanabani, Naif},
Title = {Integrated value engineering and multi-criteria decision-making process
   in BIM framework for pipe materials selection},
Journal = {KSCE JOURNAL OF CIVIL ENGINEERING},
Year = {2025},
Volume = {29},
Number = {4},
Month = {APR},
Abstract = {The choice of pipe materials is a necessary process in construction,
   influenced by numerous complex considerations. Alternative materials
   require careful study due to their differences in properties,
   performance, and cost according to the specific functions of elements.
   The Value Engineering (VE) process requires time for implementation due
   to the need for collaborative effort and multiple workshops. This study
   proposes a framework for automating the VE process for choosing
   alternative pipe materials. The framework is based on accepted standard
   tests, expert inputs, and selected agreed-upon criteria for material
   pipe measurements. The framework methodology combines the Analytic
   Hierarchy Process (AHP)/Pairwise Comparison and Function Analysis System
   Technique (FAST). The study methodology includes ranking criteria
   weights, incorporating pipe functions, and using Building Information
   Modelling (BIM) automation software. Finally, the framework's
   application is demonstrated using a case study of a potable water
   network project in Saudi Arabia. The case study has three pipe material
   alternatives - ductile iron (DI), polyethylene (PE), and
   glass-reinforced plastic (GRP). This study supports stakeholders in
   making a decision and choosing the best alternative based on quality,
   function, and cost together.},
