Title = {Geomatic techniques for utilities consumption analysis in urban areas
   during emergency periods},
Journal = {GEOMEDIA},
Year = {2023},
Volume = {27},
Number = {3},
Pages = {16-21},
Abstract = {This paper has the main purpose of proposing a methodology to understand
   the occupation of parking spots by using the synergy of different
