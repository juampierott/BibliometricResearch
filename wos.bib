@article{ WOS:001175579500001,
Author = {Li, Ao and Zhang, Jing and Xiao, Fu and Fan, Cheng and Yu, Yantao and
   Chen, Zhe},
Title = {Design information-assisted graph neural network for modeling central
   air conditioning systems},
