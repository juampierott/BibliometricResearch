@article{ WOS:001058479700001,
Author = {Bourahla, N. and Larfi, S. and Souaci, K. and Bourahla, Y. and Tafraout,
   S.},
Title = {Intelligent automation and optimization of reinforced concrete dual
   systems for earthquake resisting buildings in a BIM environment},
Journal = {JOURNAL OF BUILDING ENGINEERING},
Year = {2023},
Volume = {76},
Month = {OCT 1},
Abstract = {To keep pace with the trend of automating structural design processes,
   this paper presents an intelligent framework for generating optimized
   reinforced concrete dual systems (RCDS) based on given architectural
   configurations extracted in IFC format, allowing seamless integration
   with a BIM platform. First, a technique is developed to reduce the
   infinite number of possibilities for column locations on the
   architectural layout to a limited number of potential positions for the
   columns of the frame. Next, a hill climbing algorithm is used to define
   the optimal arrangements and axes for the columns of the building
   skeleton. Finally, a genetic algorithm with multi-objective function is
   applied to generate an optimized structural shear walls layout that fits
   within the predefined skeleton outlines. The performance and sensitivity
   of the proposed pro-cedure are evaluated using three typical
   architectural configurations. The results demonstrate that the framework
   has high capacity for generating optimized RCDSs that comply with
   pre-defined structural design criteria and fit well into the
   architectural layouts.},
